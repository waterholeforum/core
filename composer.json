{
    "name": "waterhole/core",
    "description": "Waterhole core",
    "keywords": [
        "forum"
    ],
    "license": "proprietary",
    "require": {
        "ext-json": "*",
        "composer/composer": "^2.4",
        "doctrine/dbal": "^3.1",
        "fntneves/laravel-transactional-events": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hotwired/turbo-laravel": "^1.11",
        "intervention/image": "^2.5",
        "jrmajor/fluent": "^0.4.1",
        "kirschbaum-development/eloquent-power-joins": "^3.0",
        "laminas/laminas-feed": "^2.20",
        "laravel/framework": "^10.0",
        "s9e/text-formatter": "^2.13",
        "secondnetwork/blade-tabler-icons": "^2.6",
        "staudenmeir/laravel-adjacency-list": "^1.7",
        "staudenmeir/laravel-cte": "^1.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Waterhole\\": "src/",
            "Waterhole\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "Waterhole\\Providers\\WaterholeServiceProvider"
            ],
            "aliases": {
                "Waterhole": "Waterhole\\Waterhole"
            }
        }
    }
}
